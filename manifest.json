{
    "name": "Slack Link Preview",
    "description": "Slack Link Preview",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "128": "icon.png"
    },
    "permissions": ["storage"],
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "exclude_matches": ["https://*.slack.com/*"],
            "js": ["preview.js"]
        }
    ]
}
